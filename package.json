{
  "name": "json-storage-formatter",
  "version": "2.0.1",
  "description": "Package for json stringify objects without losing data types",
  "main": "lib/bundle.js",
  "types": "lib/index.d.ts",
  "typesVersions": {
    "*": {
      "clone": ["./lib/clone.d.ts"],
      "isNil": ["./lib/isNil.d.ts"],
      "isNumber": ["./lib/isNumber.d.ts"],
      "isBoolean": ["./lib/isBoolean.d.ts"],
      "isString": ["./lib/isString.d.ts"],
      "isDate": ["./lib/isDate.d.ts"],
      "isRegex": ["./lib/isRegex.d.ts"],
      "isFunction": ["./lib/isFunction.d.ts"],
      "isPrimitive": ["./lib/isPrimitive.d.ts"],
      "types": ["./lib/types.d.ts"],
      "formatFromStore": ["./lib/formatFromStore.d.ts"],
      "formatToStore": ["./lib/formatToStore.d.ts"]
    }
  },
  "module": "lib/index.js",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    },
    "./clone": {
      "import": "./lib/clone.js",
      "require": "./lib/clone.js"
    },
    "./isNil": {
      "import": "./lib/isNil.js",
      "require": "./lib/isNil.js"
    },
    "./isNumber": {
      "import": "./lib/isNumber.js",
      "require": "./lib/isNumber.js"
    },
    "./isBoolean": {
      "import": "./lib/isBoolean.js",
      "require": "./lib/isBoolean.js"
    },
    "./isString": {
      "import": "./lib/isString.js",
      "require": "./lib/isString.js"
    },
    "./isDate": {
      "import": "./lib/isDate.js",
      "require": "./lib/isDate.js"
    },
    "./isRegex": {
      "import": "./lib/isRegex.js",
      "require": "./lib/isRegex.js"
    },
    "./isFunction": {
      "import": "./lib/isFunction.js",
      "require": "./lib/isFunction.js"
    },
    "./isPrimitive": {
      "import": "./lib/isPrimitive.js",
      "require": "./lib/isPrimitive.js"
    },
    "./json-storage-formatter.types": {
      "import": "./lib/json-storage-formatter.types.js",
      "require": "./lib/json-storage-formatter.types.js"
    },
    "./formatFromStore": {
      "import": "./lib/formatFromStore.js",
      "require": "./lib/formatFromStore.js"
    },
    "./formatToStore": {
      "import": "./lib/formatToStore.js",
      "require": "./lib/formatToStore.js"
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "test:debug": "node --inspect-brk node_modules/.bin/jest --watch --runInBand",
    "test:quick": "jest --maxWorkers=4 -c --no-watchman -u",
    "test:coverage": "jest --maxWorkers=4 -c --colors --no-watchman --verbose --coverage",
    "build": "webpack --config webpack.config.js",
    "prepare": "npm run build",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnny-quesada-developer/json-storage-formatter.git"
  },
  "keywords": [
    "json",
    "state",
    "typescript",
    "storage",
    "asynstorage",
    "localstorage"
  ],
  "author": "johnny quesada",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johnny-quesada-developer/json-storage-formatter/issues"
  },
  "homepage": "https://github.com/johnny-quesada-developer/json-storage-formatter#readme",
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-transform-modules-commonjs": "^7.21.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@types/jest": "^26.0.17",
    "@types/lodash": "^4.14.165",
    "babel-loader": "^9.1.2",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "typescript": "^4.1.2",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.0.1"
  }
}
