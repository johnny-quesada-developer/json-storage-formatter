{
  "name": "json-storage-formatter",
  "version": "3.0.2",
  "description": "Package for json stringify objects without losing data types",
  "main": "./bundle.js",
  "types": "./index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./bundle.js",
      "require": "./bundle.js",
      "types": "./index.d.ts"
    },
    "./isNil": {
      "import": "./isNil.js",
      "require": "./isNil.js",
      "types": "./isNil.d.ts"
    },
    "./isNumber": {
      "import": "./isNumber.js",
      "require": "./isNumber.js",
      "types": "./isNumber.d.ts"
    },
    "./isBoolean": {
      "import": "./isBoolean.js",
      "require": "./isBoolean.js",
      "types": "./isBoolean.d.ts"
    },
    "./isString": {
      "import": "./isString.js",
      "require": "./isString.js",
      "types": "./isString.d.ts"
    },
    "./isDate": {
      "import": "./isDate.js",
      "require": "./isDate.js",
      "types": "./isDate.d.ts"
    },
    "./isRegex": {
      "import": "./isRegex.js",
      "require": "./isRegex.js",
      "types": "./isRegex.d.ts"
    },
    "./isFunction": {
      "import": "./isFunction.js",
      "require": "./isFunction.js",
      "types": "./isFunction.d.ts"
    },
    "./isPrimitive": {
      "import": "./isPrimitive.js",
      "require": "./isPrimitive.js",
      "types": "./isPrimitive.d.ts"
    },
    "./formatFromStore": {
      "import": "./formatFromStore.js",
      "require": "./formatFromStore.js",
      "types": "./formatFromStore.d.ts"
    },
    "./formatToStore": {
      "import": "./formatToStore.js",
      "require": "./formatToStore.js",
      "types": "./formatToStore.d.ts"
    }
  },
  "files": [
    "./*.js",
    "./*.d.ts"
  ],
  "scripts": {
    "format": "prettier --write .  --log-level silent && yarn lint:fix",
    "test": "yarn build && yarn jest",
    "test:debug:no-watch": "node --inspect-brk node_modules/.bin/jest --no-watchman --runInBand",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --watch --runInBand",
    "test:quick": "yarn test --no-coverage --maxWorkers=4 -c --no-watchman -u",
    "test:coverage": "yarn test --maxWorkers=4 -c --colors --no-watchman --verbose --coverage",
    "build": "yarn ts-check && yarn clean && webpack --config webpack.config.js",
    "prepare": "yarn format && npm run build && yarn test:quick",
    "version": "npm run format && yarn build && yarn jest && git add -A",
    "postversion": "git push && git push --tags",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix --max-warnings=0",
    "clean": "find . -maxdepth 1 -type f \\( -name '*.js' -o -name '*.d.ts' \\) ! -name 'webpack.config.js' ! -name 'eslint.config.mts' -exec rm {} +",
    "ts-check": "tsc -p tsconfig.json --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnny-quesada-developer/json-storage-formatter.git"
  },
  "keywords": [
    "json",
    "state",
    "typescript",
    "storage",
    "asynstorage",
    "localstorage"
  ],
  "author": "johnny quesada",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johnny-quesada-developer/json-storage-formatter/issues"
  },
  "homepage": "https://github.com/johnny-quesada-developer/json-storage-formatter#readme",
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-transform-modules-commonjs": "^7.21.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@eslint/js": "^9.39.1",
    "@types/jest": "^26.0.17",
    "@types/lodash": "^4.14.165",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "babel-loader": "^9.1.2",
    "eslint": "^9.39.1",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^29.3.1",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "typescript": "^4.1.2",
    "typescript-eslint": "^8.46.3",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.0.1"
  }
}
