{
  "name": "json-storage-formatter",
  "version": "2.0.5",
  "description": "Package for json stringify objects without losing data types",
  "main": "./bundle.js",
  "types": "./index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./bundle.js",
      "require": "./bundle.js",
      "types": "./index.d.ts"
    },
    "./clone": {
      "import": "./clone.js",
      "require": "./clone.js",
      "types": "./clone.d.ts"
    },
    "./isNil": {
      "import": "./isNil.js",
      "require": "./isNil.js",
      "types": "./isNil.d.ts"
    },
    "./isNumber": {
      "import": "./isNumber.js",
      "require": "./isNumber.js",
      "types": "./isNumber.d.ts"
    },
    "./isBoolean": {
      "import": "./isBoolean.js",
      "require": "./isBoolean.js",
      "types": "./isBoolean.d.ts"
    },
    "./isString": {
      "import": "./isString.js",
      "require": "./isString.js",
      "types": "./isString.d.ts"
    },
    "./isDate": {
      "import": "./isDate.js",
      "require": "./isDate.js",
      "types": "./isDate.d.ts"
    },
    "./isRegex": {
      "import": "./isRegex.js",
      "require": "./isRegex.js",
      "types": "./isRegex.d.ts"
    },
    "./isFunction": {
      "import": "./isFunction.js",
      "require": "./isFunction.js",
      "types": "./isFunction.d.ts"
    },
    "./isPrimitive": {
      "import": "./isPrimitive.js",
      "require": "./isPrimitive.js",
      "types": "./isPrimitive.d.ts"
    },
    "./formatFromStore": {
      "import": "./formatFromStore.js",
      "require": "./formatFromStore.js",
      "types": "./formatFromStore.d.ts"
    },
    "./formatToStore": {
      "import": "./formatToStore.js",
      "require": "./formatToStore.js",
      "types": "./formatToStore.d.ts"
    }
  },
 "files": [
    "./*.js",
    "./*.d.ts"
  ],
  "scripts": {
    "test:debug": "node --inspect-brk node_modules/.bin/jest --watch --runInBand",
    "test:quick": "jest --maxWorkers=4 -c --no-watchman -u",
    "test:coverage": "jest --maxWorkers=4 -c --colors --no-watchman --verbose --coverage",
    "build": "webpack --config webpack.config.js",
    "prepare": "npm run build",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "clean": "rm -rf ./*.js ./*.d.ts !webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnny-quesada-developer/json-storage-formatter.git"
  },
  "keywords": [
    "json",
    "state",
    "typescript",
    "storage",
    "asynstorage",
    "localstorage"
  ],
  "author": "johnny quesada",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johnny-quesada-developer/json-storage-formatter/issues"
  },
  "homepage": "https://github.com/johnny-quesada-developer/json-storage-formatter#readme",
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-transform-modules-commonjs": "^7.21.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@types/jest": "^26.0.17",
    "@types/lodash": "^4.14.165",
    "babel-loader": "^9.1.2",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "typescript": "^4.1.2",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.0.1"
  }
}
